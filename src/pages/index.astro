---
import GroupingGrid from "../components/GroupingGrid.astro";
import BaseLayout from "../layouts/BaseLayout.astro";
import GridCard from "../components/Grid Card/GridCard";
import APITester from "../components/APITester";
const pageTitle = "Sylvan Archive";
---

<BaseLayout pageTitle={pageTitle}>
  <GridCard
    displayArt="young pyromancer"
    mainArtIdentifiers={{
      cardSet: "sld",
      cardCollectNum: 17,
      cardFace: "back",
    }}
    bgCards={["lightning bolt", "counterspell"]}
    title="Izzet Spells"
    client:visible
  />
  <GridCard
    displayArt="reanimate"
    bgCards={[
      "entomb",
      { bgCard: "dark ritual", identifiers: { cardSet: "ema" } },
      "griselbrand",
    ]}
    title="Reanimator"
    client:visible
  />
  <GridCard
    displayArt="hogaak, arisen necropolis"
    bgCards={[
      "satyr wayfinder",
      { bgCard: "altar of dementia", identifiers: { cardSet: "mh2" } },
      { bgCard: "bridge from below", identifiers: { cardSet: "uma" } },
    ]}
    title="Dredge"
    client:visible
  />
  <!-- <APITester
    client:visible
  /> -->
</BaseLayout>

<style></style>
