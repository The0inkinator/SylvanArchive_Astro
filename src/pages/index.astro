    displayArt="flooded strand"
    bgCard1="polluted delta"
    bgCard2="wooded foothills"
    bgCard3="windswept heath"
    client:visible
  />
  <GridCard
    displayArt="orcish bowmasters"
    bgCard1="the one ring"
    bgCard2="eagles of the north"
    bgCard3="lembas"
    client:visible
  />
